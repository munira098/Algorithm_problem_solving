//input arr=[6,3,-2,8,-66,13,4]
//result[-66,-2,3,4,6,8,13]

function bSort(arr){

    let swapped
    do{
        swapped=false;

    for(let i=0;i<arr.length-1;i++){
        if (arr[i]>arr[i+1]){
            let temp=arr[i];
            arr[i]=arr[i+1];
            arr[i+1]=temp;
            swapped=true;
        }

        }
        

    
    }  while(swapped);
           
     
    return { arr};

 };
